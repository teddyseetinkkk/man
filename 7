Tabs.Pull:AddSection("Mirage")

local A1 = Tabs.Pull:AddParagraph({
    Title = "Check Mirage",
    Desc = ""
})

task.spawn(function()
    while task.wait(math.random(0.05, 0.1)) do
        pcall(function()
            local B1 = game.Workspace._WorldOrigin.Locations:FindFirstChild("Mirage Island")
            local C1 = B1 and "Check Mirage : Spawn!" or "Check Mirage : Not Spawn"
            A1:SetDesc(C1)
        end)
    end
end)

local D1 = Tabs.Pull:AddToggle("Toggle", { Title = "Teleport Mirage Island", Default = false })
D1:OnChanged(function(E1)
    _G.MysticIsland = E1
    StopTween(_G.MysticIsland)
end)

spawn(function()
    pcall(function()
        while wait(math.random(0.08, 0.12)) do
            if _G.MysticIsland then
                local F1 = game:GetService("Workspace").Map:FindFirstChild("MysticIsland")
                if F1 then
                    local G1 = F1.WorldPivot.Position
                    topos(CFrame.new(G1.X, math.random(499, 501), G1.Z))
                end
            end
        end
    end)
end)

local H1 = Tabs.Pull:AddToggle("Toggle", { Title = "Auto Lock Moon", Default = false })
H1:OnChanged(function(I1)
    _G.LockCamToMoon = I1
end)

spawn(function()
    while wait(math.random(0.07, 0.11)) do
        pcall(function()
            if _G.LockCamToMoon then
                wait(0.1)
                local J1 = game.Lighting:GetMoonDirection()
                local K1 = game.Workspace.CurrentCamera.CFrame.p + J1 * 100
                game.Workspace.CurrentCamera.CFrame = CFrame.lookAt(game.Workspace.CurrentCamera.CFrame.p, K1)
            end
        end)
    end
end)

local L1 = Tabs.Pull:AddToggle("Toggle", { Title = "Tween Gear", Default = false })
L1:OnChanged(function(M1)
    _G.TweenMGear = M1
    StopTween(_G.TweenMGear)
end)

spawn(function()
    pcall(function()
        while wait(math.random(0.09, 0.13)) do
            if _G.TweenMGear then
                local N1 = game:GetService("Workspace").Map:FindFirstChild("MysticIsland")
                if N1 then
                    for _, O1 in pairs(N1:GetChildren()) do
                        if O1:IsA("MeshPart") and O1.Material == Enum.Material.Neon then
                            topos(O1.CFrame)
                        end
                    end
                end
            end
        end
    end)
end)

local A1 = Tabs.Pull:AddToggle("Auto Trial V4", { Title = "Auto Trial V4", Default = false })
A1:OnChanged(function(B1)
    _G.QuestRace = B1
    StopTween(_G.QuestRace)
end)

spawn(function()
    pcall(function()
        while wait(math.random(0.05, 0.1)) do
            if _G.QuestRace then
                local C1 = game:GetService("Players").LocalPlayer.Data.Race.Value
                if C1 == "Human" or C1 == "Ghoul" then
                    for _, D1 in pairs(game.Workspace.Enemies:GetDescendants()) do
                        if D1:FindFirstChild("Humanoid") and D1:FindFirstChild("HumanoidRootPart") and D1.Humanoid.Health > 0 then
                            pcall(function()
                                repeat wait(0.1)
                                    D1.Humanoid.Health = 0
                                    D1.HumanoidRootPart.CanCollide = false
                                    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                                until not _G.QuestRace or not D1.Parent or D1.Humanoid.Health <= 0
                            end)
                        end
                    end
                elseif C1 == "Skypiea" then
                    for _, E1 in next, workspace:GetDescendants() do
                        if E1.Name == "FinishPart" then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = E1.CFrame
                        end
                    end
                elseif C1 == "Fishman" then
                    for _, F1 in pairs(game:GetService("Workspace").SeaBeasts.SeaBeast1:GetDescendants()) do
                        if F1.Name == "HumanoidRootPart" then
                            topos(F1.CFrame * CFrame.new(PosX, PosY, PosZ))
                            for _, G1 in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                                if G1:IsA("Tool") and (G1.ToolTip == "Melee" or G1.ToolTip == "Blox Fruit" or G1.ToolTip == "Sword" or G1.ToolTip == "Gun") then
                                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(G1)
                                    for _, H1 in ipairs({ 122, 120, 99 }) do
                                        for J1 = 1, 2 do
                                            game:GetService("VirtualInputManager"):SendKeyEvent(J1 == 1, H1, false, game.Players.LocalPlayer.Character.HumanoidRootPart)
                                            wait(0.2)
                                        end
                                    end
                                end
                            end
                        end
                    end
                elseif C1 == "Cyborg" then
                    topos(CFrame.new(28654, 14898.7832, -30))
                elseif C1 == "Mink" then
                    for _, K1 in pairs(game:GetService("Workspace"):GetDescendants()) do
                        if K1.Name == "StartPoint" then
                            topos(K1.CFrame * CFrame.new(0, 10, 0))
                        end
                    end
                end
            end
        end
    end)
end)
